{
  "name": "onuxt-capacitor",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "nuxt dev",
    "dev:ssr": "nuxt prepare && NUXT_SSR=true nuxt dev",
    "dev:csr": "nuxt prepare && nuxt dev",
    "build": "nuxt generate && cap sync",
    "build:web": " NUXT_SSR=false nuxt generate",
    "build:ssr": "nuxt prepare && NUXT_SSR=true nuxt build",
    "build:csr": "nuxt prepare && nuxt generate && cap sync",
    "postinstall": "nuxt prepare && nuxt generate",
    "ionic:serve": "nuxt dev",
    "ionic:build": "nuxt generate && cap sync",
    "ios": "ionic cap run ios --livereload --external",
    "ios:sync": "nuxt prepare && nuxt generate && cap sync && ionic cap run ios --livereload --external",
    "android": "ionic cap run android --livereload --external",
    "android:sync": "nuxt prepare && nuxt generate && cap sync && ionic cap run android --livereload --external",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "@capacitor/android": "^6.0.0",
    "@capacitor/core": "^6.0.0",
    "@capacitor/ios": "^6.0.0",
    "@nuxtjs/i18n": "^10.1.0",
    "@pinia/nuxt": "^0.11.2",
    "@vant/nuxt": "^1.0.7",
    "clipboard-polyfill": "^4.1.1",
    "dayjs": "^1.11.18",
    "dompurify": "^3.3.0",
    "nuxt": "^3.19.3",
    "pinia": "^3.0.3",
    "swiper": "^12.0.2",
    "ua-parser-js": "^2.0.6",
    "vant": "^4.9.21",
    "vue": "^3.5.22",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.20.0",
    "@capacitor/cli": "^6.0.0",
    "@ionic/cli": "^7.2.0",
    "@nuxt/eslint": "^0.3.13",
    "@nuxtjs/tailwindcss": "^6.12.0",
    "@types/node": "^20.14.1",
    "eslint": "^9.4.0",
    "less": "^4.4.2",
    "typescript": "^5.4.5"
  }
}
